{
    "name": "custom/easy-http",
    "description": "A PSR-7/PSR15/PSR-17/PSR-18 compatible HTTP client",
    "keywords": ["psr", "psr-7", "psr-15", "psr-17", "psr-18", "http", "client"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Deem Moor",
            "homepage": "https://deemmoor.ru/"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Custom\\EasyHttp\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": {
        "easy-cache": {
            "type": "git",
            "url": "https://github.com/DeemMoor/easy-http.git"
        }
    }
}
